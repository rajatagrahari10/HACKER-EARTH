#include<iostream>
using namespace std;
int main()
{
    int n,i,x;
    cin>>n;
    int skils;
    cin>>skils;
    int a[n];
    
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    
     for(i=0;i<n;i++)
     {
        if(a[i]>=skils)
        {
            cout<<"YES";
        }
        else
        {
            cout<<"NO";
        }
        cout<<endl;
    }
    return 0;
    
}
